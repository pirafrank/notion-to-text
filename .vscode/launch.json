  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run AWS",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/aws.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run CLI",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/cli.js",
      "args": [ "https://pirafrank.notion.site" ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/server.js"
    }
  ]
}
