  "version": "0.2.0",
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "lambda:index.handler (nodejs14.x)",
      "invokeTarget": {
        "target": "code",
        "projectRoot": "${workspaceFolder}/lambda",
        "lambdaHandler": "index.handler"
      },
      "lambda": {
        "runtime": "nodejs14.x",
        "payload": {},
        "environmentVariables": {}
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run CLI",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/standalone/cli.js",
      "args": [
        "https://pirafrank.notion.site"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/standalone/server.js"
    }
  ]
}
